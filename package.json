{
  "name": "vue-chessboard",
  "description": "Chessboard vue component to load positions, create positions and see threats",
  "version": "0.7.0",
  "author": {
    "name": "vitogit",
    "email": "vitogit@users.noreply.github.com"
  },
  "keywords": [
    "vue",
    "vuejs",
    "chessboard",
    "chess"
  ],
  "license": "GPL-3.0",
  "main": "dist/vue-chessboard.common.js",
  "unpkg": "dist/vue-chessboard.umd.js",
  "module": "index.js",
  "scripts": {
    "serve": "yarn vue-cli-service serve",
    "build": "yarn vue-cli-service build",
    "build-lib": "yarn vue-cli-service build --target lib --name vue-chessboard ./src/index.js",
    "lint": "yarn vue-cli-service lint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vitogit/vue-chessboard.git"
  },
  "bugs": {
    "url": "https://github.com/vitogit/vue-chessboard/issues"
  },
  "homepage": "https://github.com/vitogit/vue-chessboard#readme",
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.2.0",
    "@vue/cli-plugin-eslint": "~4.2.0",
    "@vue/cli-service": "~4.2.0",
    "babel-eslint": "^10.0.3",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.1.2",
    "vue-template-compiler": "^2.6.11"
  },
  "dependencies": {
    "chess.js": "0.10.2",
    "chessground": "7.3.2"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
